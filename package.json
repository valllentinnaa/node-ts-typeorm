{
  "name": "node_ts_skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod": "NODE_ENV=production ts-node ./dist/app.js",
    "build:ts": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start:dev": "NODE_ENV=development nodemon src/app.ts",
    "start:test": "NODE_ENV=testing ts-node src/app.ts",
    "test": "NODE_ENV=testing mocha",
    "test:ci": "CI=true && npm run test",
    "typeorm:cli": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node typeorm:cli migration:create --  -n",
    "migration:generate": "NODE_ENV=development npm run typeorm:cli migration:generate --  -n",
    "migration:dev": "NODE_ENV=development npm run typeorm:cli -- migration:run",
    "migration:test": "NODE_ENV=testing npm run typeorm:cli -- migration:run",
    "migration:revert": "ts-node typeorm migration:revert",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:dev": "NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:test": "NODE_ENV=testing ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^5.5.9",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/luxon": "^2.0.1",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.29",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-spellcheck": "^0.0.19",
    "husky": "^7.0.0",
    "mocha": "^9.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.2",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.0.2",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.13.0",
    "typeorm": "^0.2.38",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run build:ts"
    }
  }
}
